---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.astro";

export function getStaticPaths() {
    return [
        {
            params: { test: "test" },
            props: {
                imgSrc: "assets/test.jpg",
            },
        },
    ];
}

const { imgSrc } = Astro.props;
---

<Layout title="test">
    <section>
        <Card imgSource={imgSrc} />
    </section>
</Layout>
